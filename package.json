{
  "name": "auth",
  "version": "1.0.0",
  "description": "authentication server",
  "type": "module",
  "scripts": {
    "development": "cross-env NODE_ENV=development node server",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr client/entry-server.jsx --outDir dist/server",
    "production": "cross-env NODE_ENV=production node server"
  },
  "keywords": [
    "jwt",
    "authentication",
    "login",
    "express",
    "mongodb"
  ],
  "author": "shubham oulkar",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.10.0",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.0",
    "sirv": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.3",
    "vite": "^5.4.9"
  }
}
