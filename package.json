{
  "name": "auth",
  "version": "1.0.0",
  "description": "authentication server",
  "type": "module",
  "scripts": {
    "clean": "rmdir dist",
    "dev": "cross-env NODE_ENV=development node --inspect server.js",
    "build": "pnpm run build:client && pnpm run build:server ",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr client/entry-server.tsx --outDir dist/server",
    "production": "cross-env NODE_ENV=production node server.js"
  },
  "keywords": [
    "jwt",
    "authentication",
    "login",
    "express",
    "mongodb",
    "SSR",
    "React"
  ],
  "author": "shubham oulkar",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "google-auth-library": "^9.15.0",
    "jose": "^5.9.6",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.10.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.0",
    "sirv": "^3.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "typescript": "~5.6.3",
    "vite": "^5.4.9"
  }
}
